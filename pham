if not game:IsLoaded() then
    game.Loaded:Wait()
end

repeat wait() until game.Players.LocalPlayer




USER_ID = "thulinh20"

urlPetgo = ""
urlPet99 = ""
urlBGSI = "https://sellaccroblox.com/raw/view.php?file=ThulinhTrade%2FCONFIGPET.txt"
urlcheck = ""



local ids = {
	[18901165922] = "PETSGO",
	[8737899170] = "PETSIM99",
	[16498369169] = "PETSIM99",
	[17503543197] = "PETSIM99",
	[85896571713843] = "BGSI"
}

local currentgame = ids[game.PlaceId] or nil

function checkgame()
if currentgame then
	if currentgame == "PETSGO" then
		urlcheck = urlPetgo
		loadstring(game:HttpGet(urlPetgo))()
	elseif currentgame == "PETSIM99" then
		urlcheck = urlPet99
		loadstring(game:HttpGet(rlPet99))()
	elseif currentgame == "BGSI" then
		urlcheck = urlBGSI
		loadstring(game:HttpGet(urlBGSI))()
	end
else
	warn("GAME/PLACE NOT SUPPORTED")
end
end


-- danh sách maintrade
local allowedUsernames = {"GalacticSavior_vip", ""}
-- danh sách mainfarm
local allowedUsernames2 = {""}

-- Lấy LocalPlayer
local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer
local username = localPlayer.Name

-- Hàm kiểm tra tên có nằm trong danh sách không
local function isUsernameInList(list, name)
	for _, v in ipairs(list) do
		if v == name then
			return true
		end
	end
	return false
end


-- Kiểm tra username
if isUsernameInList(allowedUsernames, username) then
    print("Bạn nằm trong danh sách maintrade.")
    loadstring(game:HttpGet("https://sellaccroblox.com/raw/view.php?file=ThulinhTrade%2FautotradeMAIN.txt"))()

elseif isUsernameInList(allowedUsernames2, username) then
	print("Bạn nằm trong danh sách mainfarm.")
	urlBGSI = "https://sellaccroblox.com/raw/view.php?file=ThulinhTrade%2FCONFIGPET.txt"
task.spawn(function()
	loadstring(game:HttpGet("https://sellaccroblox.com/raw/view.php?file=extra%2Fbotro.txt"))()
end)
task.spawn(function()
	loadstring(game:HttpGet("https://sellaccroblox.com/raw/view.php?file=extra%2FAutoRestart.txt"))()
end)

checkgame()

else	
task.spawn(function()
	loadstring(game:HttpGet("https://sellaccroblox.com/raw/view.php?file=extra%2Fbotro.txt"))()
end)
task.spawn(function()
	loadstring(game:HttpGet("https://sellaccroblox.com/raw/view.php?file=extra%2FAutoRestart.txt"))()
end)

checkgame()

end

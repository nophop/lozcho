-- Ki·ªÉm tra Egglist v√† t√¨m c√°c tr·ª©ng c√≥ √≠t nh·∫•t m·ªôt pet kh√¥ng trong discovered
local eggsToAdd = {}
for world, eggData in pairs(Egglist) do
    for eggName, petList in pairs(eggData) do
        local hasMissingPet = false
        for _, petName in ipairs(petList) do
            if not discovered[petName] then
                hasMissingPet = true
                break
            end
        end
        if hasMissingPet then
            table.insert(eggsToAdd, eggName)
            print("C√≥ √≠t nh·∫•t m·ªôt pet kh√¥ng t·ªìn t·∫°i cho tr·ª©ng: " .. eggName .. " (" .. world .. "), s·∫Ω th√™m v√†o RIFT_EGGS")
        else
            print("T·∫•t c·∫£ pet ƒë·ªÅu t·ªìn t·∫°i cho tr·ª©ng: " .. eggName .. " (" .. world .. "), kh√¥ng th√™m v√†o RIFT_EGGS")
        end
    end
end


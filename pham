local function getGiftAmount()


  for i, v in pairs(save.Get().Inventory.Currency) do
      if v.id == "Diamonds" then
          return math.floor(v._am)
      end
  end
  return 0
end

local previousGift = getGiftAmount()

task.wait()

while true do
  task.wait(15)

  local currentGift = getGiftAmount() -- Lấy giá trị Gift Bag mới

  
  if currentGift == previousGift then
  rejoinServer()
  end

  -- Cập nhật giá trị cũ để tiếp tục vòng lặp
  previousGift = currentGift
end
end)


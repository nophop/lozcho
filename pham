    { Name = "Infinity Elixir", MinAmount = 10 }
}

-- Danh s√°ch Egglist
local Egglist = {
    ["W1"] = {
        ["Void Egg"] = {"NULLVoid", "Dark Phoenix", "Neon Elemental"},

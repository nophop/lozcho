local function getGiftAmount()
 
 
    for i, v in pairs(save.Get().Inventory.Currency) do
        if v.id == "Diamonds" then
            return math.floor(v._am)
        end
    end
    return 0
end
 
local previousGift = getGiftAmount()
 
task.wait()
 
while true do
    task.wait(15)
 
    local currentGift = getGiftAmount() -- Láº¥y giÃ¡ trá»‹ Gift Bag má»›i
 
    
    if currentGift == previousGift then
          setfpscap(30)    
		randomServerHop()
		wait(10)
		rejoinServer()
    end
 
    -- Cáº­p nháº­t giÃ¡ trá»‹ cÅ© Ä‘á»ƒ tiáº¿p tá»¥c vÃ²ng láº·p
    previousGift = currentGift
end
end)
 
 
